<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yash.spider_task_3">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/tictactoeicon"
        android:label="Tic-Tac-Toe"
        android:roundIcon="@mipmap/tictactoeicon"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".score" />
        <activity android:name=".splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Game_Activity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".item" />
    </application>

</manifest>